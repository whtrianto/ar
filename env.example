# Environment variables for Web AR Furniture App
# Copy this file to .env.local for local development

# WebXR Configuration
REACT_APP_WEBXR_ENABLED=true
REACT_APP_MAX_MODEL_SIZE=52428800

# App Configuration
REACT_APP_APP_NAME=Web AR Furniture
REACT_APP_VERSION=1.0.0

# Development URLs (for local testing)
REACT_APP_API_URL=http://localhost:3000
REACT_APP_WS_URL=ws://localhost:3000

# Production URLs (will be set automatically by Vercel)
# REACT_APP_API_URL=https://your-app.vercel.app
# REACT_APP_WS_URL=wss://your-app.vercel.app
